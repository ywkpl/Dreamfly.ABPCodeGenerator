          "Output": {
            "Folder": "{{Name}}.Application/{{Module}}/Interface",
            "Name": "I{{Entity}}AppService.cs"
          }
        },
        {
          "File": "Templates/App/AppService.cshtml",
          "Remark": "应用服务",
          "IsExecute": true,
          "Output": {
            "Folder": "{{Name}}.Application/{{Module}}/Impl",
            "Name": "{{Entity}}AppService.cs"
          }
        },
        {
          "File": "Templates/App/CreateInputDto.cshtml",
          "Remark": "新增实体Dto",
          "IsExecute": true,
          "Output": {
            "Folder": "{{Name}}.Application/{{Module}}/Dto/{{Entity}}",
            "Name": "Create{{Entity}}Input.cs"
          }
