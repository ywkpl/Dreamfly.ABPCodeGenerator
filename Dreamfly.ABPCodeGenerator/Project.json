    "Module": "Test",
    "Name": "Newcity.ReEstate",
    "Author": {
      "Name": "ywk",
      "Email": "ywkpl@hotmail.com",
      "Remark": ""
    },
    "BuildTask": {
      "ApiProjectNames": [
        "{{Name}}.Application",
        "{{Name}}.Core",
        "{{Name}}.EntityFrameworkCore",
        "{{Name}}.Web.Host"
      ],
      "Templates": [
        {
          "File": "Templates/App/Entity.cshtml",
